// 1. Database Connection

// Ensure your .env file has a DATABASE_URL defined for your SQL Server.

datasource db {

  provider = "sqlserver"

  url      = env("DATABASE_URL")

}

// 2. Prisma Client Generator

// The client will be generated to node_modules/@prisma/client (the default path).

generator client {

  provider = "prisma-client-js"

  // The custom output line has been removed: output = "./src/generated_prisma"

}
 
model Log {

id Int @id @default(autoincrement())

title String

content String

createdAt DateTime @default(now())

}
 
 